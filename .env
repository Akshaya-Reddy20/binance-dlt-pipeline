# --- General ---
ENV=dev
BASE_URL=https://data-api.binance.vision

# Comma-separated list of symbols
SYMBOLS=BTCUSDT,ETHUSDT

# Kline interval
INTERVAL=1h

# Optional: seed the first run
START_TIME=

# Destination: clickhouse
DESTINATION=clickhouse
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_DATABASE=default
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# DLT pipeline vars
DLT_PIPELINE__DESTINATION=clickhouse
DLT_PIPELINE__CLICKHOUSE__CREDENTIALS=clickhouse://default:@localhost:9000/default
